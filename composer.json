{
  "name": "volistx/frameworkcontrol",
  "description": "A wrapper to communicate with Volistx Management API",
  "type": "library",
  "authors": [
    {
      "name": "Cryental",
      "email": "contact@cryental.dev"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-curl": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.4",
    "nesbot/carbon": "^2.58"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Volistx\\FrameworkControl\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Volistx\\FrameworkControl\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Volistx\\FrameworkControl\\FrameworkControlServiceProvider"
      ]
    }
  }
}
